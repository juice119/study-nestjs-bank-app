{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testRegex": "/test/super/(.*|(\\.|/)(test|spec))\\.ts?$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "allowJs": false,
  "testEnvironment": "node",
  "runInBand": true
}
